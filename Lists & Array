using System;

public class Tutorial
{
  public static void Main()
  {
  // write your code here
  string[] fruits = new string[3];

  fruits[0] = "apple";
  fruits[1] ="banana";
  fruits[2] = "orange";
// test code
  Console.WriteLine(fruits[0]);
  Console.WriteLine(fruits[1]);
  Console.WriteLine(fruits[2]);
  }
}
